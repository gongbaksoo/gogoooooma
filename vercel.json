{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/files/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/upload/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/chat/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/custom/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/admin/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/logs/(.*)",
            "dest": "backend/main.py"
        }
    ]
}